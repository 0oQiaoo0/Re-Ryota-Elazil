  dialoguePieces:
  - character: 0
    text: "\u665A\u4E0A\u597D\u554A\u3002"
    options: 0100000002000000
  - character: 1
    text: "\u665A\u4E0A\u597D\u3002"
    options: 03000000
  - character: 1
    text: "Elazil\u3002"
    options: 03000000
  - character: 0
    text: "\u55EF\uFF0C\u4ECA\u5929Ryota\u6709\u53BB\u770B\u4E4B\u524D\u8BF4\u7684\u70DF\u82B1\u5417\uFF1F"
    options: 
  - character: 1
    text: "\u70DF\u82B1\u8FD8\u6CA1\u6709\u5F00\u59CB\u3002"
    options: 
  - character: 0
    text: "\u8FD9\u6837\u554A\u2026\u2026"
    options: 
  - character: 0
    text: "\u542C\u6211\u8BF4\u54E6\uFF0CRyota\uFF0C\u6628\u5929\u548C\u4F60\u804A\u8FC7\u4E4B\u540E\uFF0C\u665A\u4E0A\u7761\u5F97\u7279\u522B\u5B89\u7A33\u6B38\u3002"
    options: 
  - character: 0
    text: "\u4E00\u70B9\u4E5F\u6CA1\u6709\u63D0\u9632\u5FC3\u5730\u7761\u7740\u4E86\uFF0C\u4E00\u89C9\u7761\u5230\u5927\u5929\u4EAE\u3002"
    options: 
  - character: 0
    text: "\u8FD8\u597D\u6CA1\u6709\u88AB\u866B\u65CF\u88AD\u51FB\uFF0C\u4E0D\u7136\u611F\u89C9\u4ECA\u5929\u5C31\u6CA1\u529E\u6CD5\u548C\u4F60\u8BF4\u8BDD\u4E86\uFF01"
    options: 090000000a000000
  - character: 1
    text: "\u771F\u662F\u5371\u9669\u554A\uFF01"
    options: 0b000000
  - character: 1
    text: "\u4ECA\u5929\u4E5F\u8981\u7761\u4E2A\u597D\u89C9\u54E6\u3002"
    options: 0d000000
  - character: 0
    text: "\u5C31\u662F\u8BF4\u554A\uFF0C\u4E0D\u8FC7\u8FD9\u662F\u4E0D\u662F\u4E5F\u53EF\u4EE5\u8BF4\u660E\u6211\u7684\u8FD0\u6C14\u8FD8\u4E0D\u9519\u5462\uFF1F"
    options: 
  - character: 0
    text: "\u4ECA\u5929\u662F\u6211\u7684\u5E78\u8FD0\u65E5\u4E5F\u8BF4\u4E0D\u5B9A\u3002"
    options: 0f000000
  - character: 0
    text: "\u90A3\u5F53\u7136\u4E86\uFF0C\u4ECA\u5929\u665A\u4E0A\u80AF\u5B9A\u4F1A\u7761\u4E00\u4E2A\u597D\u89C9\u7684\u3002"
    options: 
  - character: 0
    text: "\u80AF\u5B9A\u7684\u3002"
    options: 
  - character: 0
    text: "\u6211\u73B0\u5728\u6B63\u5728\u5F00\u8F66\u53BB\u4EFB\u52A1\u76EE\u7684\u5730\uFF0C\u8DEF\u4E0A\u7279\u522B\u65E0\u804A\u554A\u3002"
    options: 
  - character: 0
    text: "\u6211\u4EEC\u6765\u73A9\u70B9\u4EC0\u4E48\u5427\uFF0C\u73A9\u4EC0\u4E48\u5462\uFF1F"
    options: 
  - character: 0
    text: "\u6211\u4EEC\u6765\u73A9\u63D0\u95EE\u6E38\u620F\u5427\uFF0C\u600E\u4E48\u6837\uFF1F"
    options: 
  - character: 0
    text: "\u5C31\u662F\u6211\u6765\u63D0\u95EE\u4F60\u6765\u56DE\u7B54\u7684\u90A3\u79CD\uFF01"
    options: 1300000014000000
  - character: 1
    text: "\u542C\u8D77\u6765\u5F88\u6709\u610F\u601D\u3002"
    options: 15000000
  - character: 1
    text: "\u4E0D\u516C\u5E73\uFF01\u4E0D\u516C\u5E73\uFF01"
    options: 16000000
  - character: 0
    text: "\u5BF9\u5427\u5BF9\u5427\uFF01\u90A3\u6211\u4EEC\u5F00\u59CB\u5427\uFF01"
    options: 18000000
  - character: 0
    text: "\u771F\u662F\u7684\uFF0C\u50CF\u4E2A\u5C0F\u5B69\u5B50\u4E00\u6837\uFF01"
    options: 
  - character: 0
    text: "\u7B49\u6211\u95EE\u5B8C\u4E86\u6211\u5C31\u56DE\u7B54\u4F60\u7684\u95EE\u9898\u597D\u5566\uFF01"
    options: 
  - character: 0
    text: "\u54B3\u54B3\uFF0C\u7B2C\u4E00\u4E2A\u95EE\u9898\u54E6\uFF0C\u4F60\u559C\u6B22\u732B\u8FD8\u662F\u72D7\u5462\uFF1F"
    options: 
  - character: 0
    text: "\u867D\u7136\u6211\u4E5F\u6CA1\u600E\u4E48\u63A5\u89E6\u8FC7\u732B\u732B\u72D7\u72D7\u5C31\u662F\u4E86\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u5BF9\uFF0C\u8FD9\u4E2A\u95EE\u9898\u662F\u95EE\u4F60\u7684\uFF0C\u5FEB\u56DE\u7B54\u5566\uFF01"
    options: 1b0000001c000000
  - character: 1
    text: "\u72D7\u3002"
    options: 1d000000
  - character: 1
    text: "\u732B\u3002"
    options: 1f000000
  - character: 0
    text: "\u6211\u4E5F\u66F4\u559C\u6B22\u72D7\u54E6\uFF0C\u5E94\u8BE5\u3002"
    options: 
  - character: 0
    text: "\u6BD5\u7ADF\u72D7\u72D7\u4F1A\u66F4\u542C\u8BDD\u4E00\u4E9B\u4E9B\u5427\uFF01\u5E94\u8BE5\uFF01"
    options: 21000000
  - character: 0
    text: "\u55EF\u2026\u2026\u732B\u4E5F\u4E0D\u9519\u5566\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u732B\u54AA\u53EF\u662F\u5F88\u5435\u7684\u54E6\uFF0C\u611F\u89C9\uFF0C\u56E0\u4E3A\u4F1A\u8DD1\u6765\u8DD1\u53BB\uFF01"
    options: 
  - character: 0
    text: "\u7B2C\u4E8C\u4E2A\u95EE\u9898\u54E6\uFF0C\u4F60\u62C5\u5FC3\u81EA\u5DF1\u7684\u4EBA\u9645\u5173\u7CFB\u5417\uFF1F"
    options: 
  - character: 0
    text: "\u771F\u7684\u4F1A\u6709\u4EBA\u62C5\u5FC3\u8FD9\u4E2A\u5417\uFF1F"
    options: 2300000024000000
  - character: 1
    text: "\u62C5\u5FC3\u3002"
    options: 25000000
  - character: 1
    text: "\u4E0D\u62C5\u5FC3\u3002"
    options: 27000000
  - character: 0
    text: "\u771F\u7684\u5417\uFF1F\u6211\u89C9\u5F97\u8FD9\u4E2A\u4E0D\u9700\u8981\u62C5\u5FC3\u54E6\u3002"
    options: 
  - character: 0
    text: "\u56E0\u4E3A\u6709\u6211\u6765\u5F53\u4F60\u7684\u670B\u53CB\u561B\uFF01\u65E0\u804A\u7684\u65F6\u5019\u5C31\u53EF\u4EE5\u6765\u627E\u6211\u5C31\u662F\u4E86\uFF01"
    options: 28000000
  - character: 0
    text: "\u5BF9\u5427\u5BF9\u5427\uFF0C\u679C\u7136\u6211\u4EEC\u662F\u540C\u7C7B\u554A\uFF01"
    options: 
  - character: 0
    text: "\u7B2C\u4E09\u4E2A\u95EE\u9898\u4E86\u54E6\uFF0C\u4F60\u5BB3\u6015\u4ED6\u4EBA\u6000\u7591\u81EA\u5DF1\u5417\uFF1F"
    options: 290000002a000000
  - character: 0
    text: "\u5BB3\u6015\u3002"
    options: 2b000000
  - character: 1
    text: "\u4E0D\u5BB3\u6015\u3002"
    options: 2f000000
  - character: 0
    text: "\u8FD9\u6837\u554A\uFF0C\u8FD9\u4E5F\u662F\u5F88\u6B63\u5E38\u7684\u5566\u3002"
    options: 
  - character: 0
    text: "\u8FD9\u79CD\u4E8B\u60C5\u6211\u4E5F\u4F1A\u5BB3\u6015\u7684\uFF0C\u4EE5\u524D\u5728\u519B\u6821\u4E0A\u5B66\u7684\u65F6\u5019\u5C31\u662F\u3002"
    options: 
  - character: 0
    text: "\u5F53\u65F6\u73ED\u4E0A\u6709\u4E2A\u4EBA\u4E22\u4E86\u4E00\u4E2A\u9762\u5305\uFF0C\u5927\u5BB6\u90FD\u6000\u7591\u662F\u4E00\u4E2A\u4E2A\u5B50\u77EE\u77EE\u7684\u5BB6\u4F19\u5077\u7684\uFF0C\u7136\u540E\u5927\u5BB6\u5C31\u4E00\u8D77\u6307\u8D23\u4ED6\u4EC0\u4E48\u7684\u3002"
    options: 
  - character: 0
    text: "\u88AB\u6000\u7591\u771F\u7684\u662F\u4E00\u4EF6\u5F88\u53EF\u6015\u7684\u4E8B\u60C5\u554A\uFF01"
    options: 31000000
  - character: 0
    text: "\u771F\u7684\u5417\uFF1FRyota\u771F\u662F\u4E2A\u5389\u5BB3\u7684\u4EBA\u554A\uFF01"
    options: 
  - character: 0
    text: "\u6211\u8FD8\u4EE5\u4E3A\u6240\u6709\u4EBA\u90FD\u4F1A\u5BB3\u6015\u88AB\u8EAB\u8FB9\u7684\u4EBA\u6000\u7591\u7684\uFF0C\u7279\u522B\u662F\u670B\u53CB\u6216\u8005\u662F\u4EB2\u4EBA\u4EC0\u4E48\u7684\u3002"
    options: 
  - character: 0
    text: "\u7B2C\u56DB\u9898\u54AF\uFF01\u4F60\u662F\u559C\u6B22\u767D\u5929\u8FD8\u662F\u9ED1\u591C\u5462\uFF1F"
    options: 3200000033000000
  - character: 1
    text: "\u767D\u5929\u5427\u3002"
    options: 34000000
  - character: 1
    text: "\u5F53\u7136\u662F\u9ED1\u591C\uFF01"
    options: 36000000
  - character: 0
    text: "\u767D\u5929\u554A\uFF0C\u767D\u5929\u786E\u5B9E\u5F88\u597D\uFF0C\u4EAE\u5802\u5802\u7684\u3002"
    options: 
  - character: 0
    text: "\u767D\u5929\u53EF\u4EE5\u5403\u996D\uFF0C\u53EF\u4EE5\u4E0D\u7528\u706F\u5149\u5C31\u80FD\u770B\u6E05\u5468\u56F4\uFF0C\u5927\u5730\u662F\u6709\u989C\u8272\u7684\uFF0C\u5929\u7A7A\u4E5F\u662F\u3002"
    options: 39000000
  - character: 0
    text: "\u9ED1\u591C\u4E5F\u5F88\u597D\uFF0C\u5F88\u9002\u5408\u7D2F\u4E86\u4E00\u5929\u7684\u6211\u4EEC\u597D\u597D\u4F11\u606F\u3002"
    options: 
  - character: 0
    text: "\u800C\u4E14\u5230\u4E86\u4EC0\u4E48\u90FD\u770B\u4E0D\u6E05\u7684\u65F6\u5019\uFF0C\u8111\u5B50\u53CD\u800C\u4F1A\u8F6C\u5F97\u5F88\u5FEB\uFF0C\u53EF\u4EE5\u60F3\u60F3\u7B2C\u4E8C\u5929\u5E94\u8BE5\u505A\u7684\u4E8B\u60C5\u3002"
    options: 
  - character: 0
    text: "\u53EA\u662F\u591C\u665A\u7684\u5927\u5730\u6CA1\u6709\u989C\u8272\uFF0C\u5929\u7A7A\u4E5F\u662F\u3002"
    options: 
  - character: 0
    text: "\u5012\u6570\u7B2C\u4E8C\u9898\u4E86\uFF0C\u90A3\u4E48Ryota\uFF0C\u5728\u60B2\u4F24\u548C\u865A\u65E0\u4E4B\u95F4\uFF0C\u4F60\u9009\u62E9\u54EA\u4E00\u4E2A\uFF1F"
    options: 3a0000003b000000
  - character: 1
    text: "\u6211\u9009\u62E9\u60B2\u4F24\u3002"
    options: 3c000000
  - character: 1
    text: "\u6211\u9009\u62E9\u865A\u65E0\u3002"
    options: 3e000000
  - character: 0
    text: "\u6BD4\u8D77\u865A\u65E0\u66F4\u613F\u610F\u9009\u62E9\u60B2\u4F24\u5417\uFF1F\u6BD4\u8D77\u611F\u89C9\u4E0D\u5230\u81EA\u5DF1\u7684\u5B58\u5728\uFF0C\u5B81\u613F\u75DB\u82E6\u5730\u6D3B\u4E0B\u53BB\u5417\uFF1F"
    options: 
  - character: 0
    text: "\u9192\u6765\u4E4B\u540E\u7684\u75DB\u82E6\u6211\u4EEC\u771F\u7684\u80FD\u591F\u627F\u53D7\u5417\uFF1F"
    options: 40000000
  - character: 0
    text: "\u6BD4\u8D77\u60B2\u4F24\u66F4\u613F\u610F\u9009\u62E9\u865A\u65E0\u5417\uFF1F"
    options: 
  - character: 0
    text: "\u865A\u65E0\u672C\u8EAB\u5176\u5B9E\u4E5F\u662F\u4E00\u79CD\u60B2\u4F24\u4E5F\u8BF4\u4E0D\u5B9A\uFF0C\u53EA\u662F\u8FD9\u79CD\u60B2\u4F24\u5E76\u975E\u5FEB\u4E50\u7684\u5BF9\u7ACB\u9762\uFF0C\u800C\u662F\u865A\u65E0\u89E6\u89D2\u7684\u5EF6\u4F38\u3002"
    options: 
  - character: 0
    text: "\u597D\u5566\u597D\u5566\uFF0C\u6700\u540E\u4E00\u9898\u4E86\uFF01\u55EF\u2026\u2026\u6211\u6765\u770B\u770B\u2026\u2026"
    options: 
  - character: 0
    text: "\u4F60\uFF0CRyota\u4F60\u559C\u6B22\u5185\u5411\u7684\u5973\u5B69\u5B50\u8FD8\u662F\u5916\u5411\u7684\u5973\u5B69\u5B50\uFF1F\uFF01"
    options: 
  - character: 0
    text: "\u5148\u8BF4\u597D\uFF0C\u8FD9\u4E0D\u662F\u6211\u60F3\u95EE\u7684\u54E6\uFF0C\u8FD9\u662F\u63D0\u95EE\u6E38\u620F\u91CC\u81EA\u5E26\u7684\u3002"
    options: 4300000044000000
  - character: 1
    text: "\u5185\u5411\u7684\u3002"
    options: 45000000
  - character: 1
    text: "\u5916\u5411\u7684\u3002"
    options: 48000000
  - character: 0
    text: "\u6B38\uFF1F\u5185\u5411\u7684\u5973\u5B69\u5B50\u561B\uFF1F\u90A3\u6709\u4EC0\u4E48\u597D\u7684\u554A\uFF01"
    options: 
  - character: 0
    text: "\u6211\u89C9\u5F97\u5427\uFF0C\u5916\u5411\u4E00\u70B9\u7684\u5973\u5B69\u5B50\u6BD4\u8F83\u9002\u5408\u4F60\u54E6\uFF0C\u5F53\u7136\u8FD9\u4E0D\u662F\u6211\u5728\u81EA\u5938\u5566\uFF0C\u6211\u4E5F\u4E0D\u662F\u8BF4\u81EA\u5DF1\u662F\u5916\u5411\u7684\u7C7B\u578B\u5566\uFF01"
    options: 
  - character: 0
    text: "\u5F53\u7136\u8FD9\u4E9B\u90FD\u662F\u6211\u7684\u5EFA\u8BAE\u5566\uFF0C\u5E76\u4E0D\u662F\u8BF4\u771F\u7684\u53EB\u4F60\u53BB\u559C\u6B22\u81EA\u5DF1\u4E0D\u559C\u6B22\u7684\u7C7B\u578B\u3002"
    options: 4b000000
  - character: 0
    text: "\u771F\u7684\u5417\uFF1F\uFF01"
    options: 
  - character: 0
    text: "\u55EF\u54FC\u54FC\uFF0C\u679C\u7136Ryota\u662F\u4E2A\u5F88\u6709\u773C\u5149\u7684\u5BB6\u4F19\u554A\u3002"
    options: 
  - character: 0
    text: "\u6211\u4E5F\u4E0D\u662F\u8BF4\u56E0\u4E3A\u6211\u81EA\u5DF1\u662F\u6BD4\u8F83\u5916\u5411\u7684\u5973\u751F\u624D\u5F00\u5FC3\u7684\u5566\uFF01"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\uFF0C\u600E\u4E48\u6837\uFF1F\u8FD9\u4E9B\u95EE\u9898\u662F\u4E0D\u662F\u5F88\u6709\u542B\u91D1\u91CF\uFF1F\u76F4\u63A5\u51FB\u4E2D\u4E86\u4F60\u7684\u5185\u5FC3\u4EC0\u4E48\u7684\uFF1F"
    options: 4c0000004d000000
  - character: 1
    text: "\u662F\u554A\u662F\u554A\u3002"
    options: 4e000000
  - character: 1
    text: "\u5E76\u6CA1\u6709\u4EC0\u4E48\u542B\u91D1\u91CF\uFF01"
    options: 4f000000
  - character: 0
    text: "\u5BF9\u5427\u5BF9\u5427\u3002"
    options: 50000000
  - character: 0
    text: "\u6B38\u2026\u2026\u8FD9\u6837\u7684\u5417\uFF1F\u4F5C\u4E3A\u6E38\u620F\u6765\u8BF4\u4E0D\u662F\u5F88\u6709\u8DA3\u5417\uFF1F"
    options: 
  - character: 0
    text: "\u542C\u6211\u8BF4\u54E6\uFF0C\u5176\u5B9E\u662F\u6211\u767D\u5929\u7684\u65F6\u5019\u6361\u5230\u4E86\u4E00\u672C\u4E66\uFF0C\u91CC\u9762\u8FD8\u6709\u51E0\u9762\u5C45\u7136\u8FD8\u80FD\u770B\u3002"
    options: 
  - character: 0
    text: "\u6211\u521A\u624D\u95EE\u7684\u95EE\u9898\u90FD\u662F\u91CC\u9762\u7684\uFF0C\u597D\u50CF\u662F\u5FC3\u7406\u6D4B\u8BD5\uFF0C\u8FD8\u662F\u604B\u7231\u6D4B\u8BD5\u6765\u7740\u3002"
    options: 
  - character: 0
    text: "Ryota\u7684\u4E16\u754C\u6709\u7C7B\u4F3C\u7684\u4E1C\u897F\u5417\uFF1F\u6D4B\u8BD5\u4EC0\u4E48\u7684\uFF1F"
    options: 5300000054000000
  - character: 1
    text: "\u6709\u554A\u3002"
    options: 55000000
  - character: 1
    text: "\u6709\u66F4\u5389\u5BB3\u7684\u4E1C\u897F\uFF01"
