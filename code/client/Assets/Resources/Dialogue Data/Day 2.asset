  dialoguePieces:
  - character: 0
    text: "\u665A\u4E0A\u597D\u554A\uFF0CRyota\uFF0C\u4ECA\u5929\u4E5F\u6210\u529F\u8FDE\u4E0A\u4F60\u7684\u4FE1\u53F7\u4E86\u5462\u3002"
    options: 0100000002000000
  - character: 1
    text: "\u665A\u4E0A\u597D\u3002"
    options: 03000000
  - character: 1
    text: "\u5C45\u7136\u53C8\u662F\u4F60\u3002"
    options: 03000000
  - character: 0
    text: "\u4EC0\u4E48\u53EB\u53C8\u662F\u6211\u554A\uFF01\u660E\u660E\u6628\u5929\u8BF4\u597D\u7684\u4ECA\u5929\u665A\u4E0A\u7EE7\u7EED\u804A\u5929\u7684\u554A\u3002"
    options: 
  - character: 0
    text: "\u8BF4\u8D77\u6765Ryota\u5E73\u65F6\u559C\u6B22\u753B\u753B\u5417\uFF1F\u6211\u53EF\u662F\u5F88\u559C\u6B22\u54E6\u3002"
    options: 0500000006000000
  - character: 1
    text: "\u559C\u6B22\uFF01"
    options: 07000000
  - character: 1
    text: "\u4E00\u822C\u3002"
    options: 08000000
  - character: 0
    text: "\u771F\u7684\u5417\uFF1F\u592A\u68D2\u4E86\uFF01\u7ED9\u4F60\u770B\u6211\u753B\u7684\uFF01"
    options: 09000000
  - character: 0
    text: "\u6B38\uFF1F\u8FD9\u6837\u554A\uFF0C\u4E0D\u8FC7\u8FD8\u662F\u7ED9\u4F60\u770B\u4E00\u4E0B\u6211\u7684\u753B\u5427\u3002"
    options: 
  - character: 0
    text: "<color=#0066FE><u><link=\"id_02\">\u3010\u56FE\u7247\u3011</link></u>"
    options: 
  - character: 1
    text: "\u2026\u2026"
    options: 
  - character: 0
    text: "\u2026\u2026"
    options: 
  - character: 0
    text: "\u600E\u4E48\u6837\uFF1F\u5FEB\u7ED9\u70B9\u56DE\u5E94\uFF01"
    options: 0d0000000e000000
  - character: 1
    text: "\u753B\u7684\u5F88\u68D2\uFF01"
    options: 0f000000
  - character: 1
    text: "\u5B8C\u5168\u4E0D\u884C\uFF01\u662F\u5C0F\u5B69\u5B50\u7684\u6C34\u5E73\u5462\uFF01"
    options: 11000000
  - character: 0
    text: "\u771F\u7684\u5417\uFF1F\u5C31\u7B97\u4E0D\u7528\u8FD9\u79CD\u8BF4\u6CD5\u6211\u4E5F\u4E0D\u4F1A\u751F\u6C14\u7684\u5566\uFF01"
    options: 
  - character: 0
    text: "\u5176\u5B9E\u6211\u77E5\u9053\u81EA\u5DF1\u753B\u5F97\u5F88\u70C2\u5566\uFF01"
    options: 12000000
  - character: 0
    text: "\u54C8\u54C8\u54C8\uFF0C\u5BF9\u5427\u5BF9\u5427\uFF01\u5927\u5BB6\u90FD\u662F\u8FD9\u4E48\u8BF4\u7684\u54E6\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u5C31\u7B97\u662F\u8FD9\u6837\uFF0C\u6211\u8FD8\u662F\u5F88\u559C\u6B22\u753B\u753B\u3002"
    options: 
  - character: 0
    text: "\u56E0\u4E3A\u5728\u753B\u753B\u7684\u65F6\u5019\u5C31\u53EF\u4EE5\u5B8C\u5168\u653E\u7A7A\u81EA\u5DF1\u4E86\u3002"
    options: 
  - character: 0
    text: "\u8111\u5B50\u91CC\u5C31\u4F1A\u544A\u8BC9\u81EA\u5DF1\uFF0C\u8FD9\u6837\u7684\u4E16\u754C\u5F88\u4E0D\u9519\u5427\uFF0C\u7136\u540E\u624B\u5C31\u4F1A\u628A\u5B83\u753B\u4E0B\u6765\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u56E0\u4E3A\u753B\u5F97\u592A\u5DEE\u4E86\uFF0C\u5F88\u591A\u65F6\u5019\u5C31\u53EA\u6709\u6211\u4E00\u4E2A\u4EBA\u770B\u5F97\u61C2\u5462\u3002"
    options: 
  - character: 0
    text: "\u8FD9\u4E2A\u53EF\u80FD\u5C31\u662F\u552F\u4E00\u70E6\u607C\u7684\u4E8B\u60C5\u4E86\u5427\u3002"
    options: 1700000018000000
  - character: 1
    text: "\u6CA1\u6709\u5173\u7CFB\u7684\uFF0C\u53EF\u4EE5\u5F53\u505A\u5BC6\u7801\u4E00\u6837\u7684\u4E1C\u897F\u3002"
    options: 19000000
  - character: 1
    text: "\u6CA1\u6709\u5173\u7CFB\u7684\uFF0C\u81EA\u5DF1\u5F00\u5FC3\u5C31\u597D\u3002"
    options: 1a000000
  - character: 0
    text: "Ryota\u771F\u662F\u7684\uFF01\u4E0D\u8981\u62FF\u522B\u4EBA\u7684\u70E6\u607C\u5F00\u73A9\u7B11\u554A\uFF01"
    options: 1b000000
  - character: 0
    text: "\u55EF\uFF0C\u4E0D\u8FC7\u8FD8\u662F\u5E0C\u671B\u522B\u4EBA\u4E5F\u80FD\u770B\u7684\u61C2\u554A\u3002"
    options: 
  - character: 0
    text: "\u5176\u5B9E\u753B\u753B\u662F\u6211\u4ECE\u5C0F\u65F6\u5019\u5F00\u59CB\u5C31\u5F88\u559C\u6B22\u7684\u4E8B\u60C5\u4E86\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u6BD4\u8D77\u8BA4\u771F\u5730\u53BB\u7EC3\u4E60\uFF0C\u6211\u8FD8\u662F\u66F4\u52A0\u559C\u6B22\u968F\u5FC3\u6240\u6B32\u5730\u6D82\u9E26\u3002"
    options: 
  - character: 0
    text: "\u6240\u4EE5\u753B\u4E86\u8FD9\u4E48\u4E45\u6C34\u5E73\u90FD\u8FD8\u662F\u8FD9\u6837\u3002"
    options: 
  - character: 0
    text: "\u8BF4\u8D77\u6765Ryota\u6709\u4EC0\u4E48\u7231\u597D\u5417\uFF1F"
    options: 1f00000020000000
  - character: 1
    text: "\u4EE5\u524D\u662F\u6709\u7684\u3002"
    options: 21000000
  - character: 1
    text: "\u4E0D\u8FC7\u73B0\u5728\u5DF2\u7ECF\u653E\u5F03\u4E86\u3002"
    options: 21000000
  - character: 0
    text: "\u554A\uFF0C\u5DF2\u7ECF\u653E\u5F03\u4E86\u5417\uFF1F\u4E3A\u4EC0\u4E48\u554A\uFF1F"
    options: 2200000023000000
  - character: 1
    text: "\u6CA1\u6709\u65F6\u95F4\u53BB\u505A\u90A3\u4E9B\u4E8B\u60C5\u4E86\u3002"
    options: 24000000
  - character: 1
    text: "\u73B0\u5728\u6CA1\u6709\u5F53\u521D\u7684\u90A3\u79CD\u70ED\u60C5\u4E86\u3002"
    options: 24000000
  - character: 0
    text: "\u8FD9\u6837\u554A\uFF0C\u90A3\u4E5F\u662F\u6CA1\u6709\u529E\u6CD5\u7684\u4E8B\u60C5\u5462\u3002"
    options: 
  - character: 0
    text: "\u5176\u5B9E\u6211\u5012\u662F\u8D8A\u5FD9\u788C\u6216\u8005\u8D8A\u7D27\u5F20\u7684\u65F6\u5019\u5C31\u8D8A\u60F3\u8981\u53BB\u753B\u753B\u3002"
    options: 
  - character: 0
    text: "\u7ECF\u5E38\u56E0\u4E3A\u5728\u8425\u5730\u91CC\u5077\u61D2\u753B\u753B\u800C\u88AB\u961F\u957F\u9A82\u3002"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u6211\u89C9\u5F97\u5927\u6982\u7387\u662F\u56E0\u4E3A\u6211\u628A\u5979\u753B\u5F97\u592A\u4E11\u4E86\u800C\u751F\u6C14\u4E86\u5427\uFF01"
    options: 
  - character: 0
    text: "\u505A\u559C\u6B22\u7684\u4E8B\u60C5\u80FD\u8BA9\u4EBA\u4ECE\u538B\u529B\u4E2D\u89E3\u8131\u51FA\u6765\u5462\uFF0C\u4E0D\u8FC7\u5230\u6700\u540E\u8FD8\u662F\u8981\u9762\u5BF9\u538B\u529B\u4F1A\u5E26\u6765\u66F4\u75DB\u82E6\u7684\u611F\u89C9\u5462\u3002"
    options: 290000002a000000
  - character: 1
    text: "\u6DF1\u6709\u540C\u611F\u3002"
    options: 2b000000
  - character: 1
    text: "\u6211\u5012\u662F\u4E0D\u89C9\u5F97\u3002"
    options: 2b000000
  - character: 0
    text: "\u4E0D\u8FC7\u6211\u4E5F\u80AF\u5B9A\u4F1A\u597D\u597D\u5B8C\u6210\u5DE5\u4F5C\u7684\uFF0C\u4ECA\u5929\u4E5F\u662F\uFF01"
    options: 2c0000002d000000
  - character: 1
    text: "\u8FD9\u662F\u90A3\u4E48\u503C\u5F97\u5938\u8D5E\u7684\u4E8B\u60C5\u5417\uFF1F"
    options: 2e000000
  - character: 1
    text: "\u771F\u662F\u5389\u5BB3\u554A\uFF0C\u80FD\u628A\u5DE5\u4F5C\u5B8C\u6210\u3002"
    options: 30000000
  - character: 0
    text: "\u4E0D\u8981\u5C0F\u77A7\u6211\u7684\u5DE5\u4F5C\u554A\uFF0C\u6211\u6BCF\u5929\u53EF\u90FD\u662F\u6000\u7740\u5FC5\u6B7B\u7684\u51B3\u5FC3\u5728\u5DE5\u4F5C\u54E6\u3002"
    options: 
  - character: 1
    text: "\u6211\u6BCF\u5929\u4E5F\u662F\u6000\u7740\u5FC5\u6B7B\u7684\u51B3\u5FC3\u5728\u5DE5\u4F5C\u554A\u3002"
    options: 31000000
  - character: 0
    text: "\u662F\u5427\u662F\u5427\uFF01"
    options: 
  - character: 0
    text: "\u867D\u7136\u6211\u662F\u4E00\u4E2A\u88C5\u7532\u7EF4\u4FEE\u5E08\uFF0C\u4E0D\u8FC7\u90A3\u5E94\u8BE5\u7B97\u662F\u6211\u4E4B\u524D\u7684\u5DE5\u4F5C\u4E86\u3002"
    options: 
  - character: 1
    text: "\u90A3\u4F60\u73B0\u5728\u505A\u7684\u662F\u4EC0\u4E48\uFF1F"
    options: 
  - character: 0
    text: "\u73B0\u5728\u7684\u8BDD\u2026\u2026"
    options: 
  - character: 0
    text: "\u73B0\u5728\u5728\u8FD9\u7247\u5927\u5730\u4E0A\u9664\u4E86\u6211\u548C\u6211\u7684\u88C5\u7532\u5E94\u8BE5\u5C31\u6CA1\u6709\u5176\u4ED6\u4EBA\u4E86\u5427\u3002"
    options: 
  - character: 0
    text: "\u6240\u4EE5\u53EA\u8981\u5C0F\u5FC3\u4E00\u4E9B\u4F7F\u7528\u8FD9\u53F0\u673A\u4F53\u7684\u8BDD\uFF0C\u5012\u4E5F\u6CA1\u6709\u4EC0\u4E48\u7528\u5F97\u5230\u7EF4\u4FEE\u6280\u672F\u7684\u5730\u65B9\u3002"
    options: 
  - character: 0
    text: "\u6211\u73B0\u5728\u7684\u4EFB\u52A1\u7684\u8BDD\uFF0C\u5C31\u662F\u8981\u53BB\u56DE\u6536\u4E4B\u524D\u63A9\u62A4\u6211\u7684\u961F\u53CB\u7684\u673A\u5668\u6B8B\u9AB8\u3002"
    options: 
  - character: 0
    text: "\u867D\u7136\u6628\u5929\u6211\u8DDF\u4F60\u8BF4\u76F8\u4FE1\u4ED6\u4EEC\u8FD8\u6D3B\u7740\uFF0C\u4F46\u5176\u5B9E\u53EA\u662F\u5728\u901E\u5F3A\u800C\u5DF2\u3002"
    options: 
  - character: 0
    text: "\u6211\u662F\u770B\u7740\u4ED6\u4EEC\u7684\u673A\u4F53\u5728\u706B\u7130\u91CC\u88AB\u866B\u7FA4\u6495\u88C2\u7684\u3002"
    options: 
  - character: 0
    text: "\u6211\u8FD8\u770B\u5230\u4E86\u91CC\u9762\u6D41\u51FA\u6765\u7684\u50CF\u8840\u4E00\u6837\u6A59\u7EA2\u8272\u7684\u673A\u6CB9\u3002"
    options: 
  - character: 0
    text: "\u5728\u6CA1\u6709\u88C5\u7532\u4FDD\u62A4\u7684\u60C5\u51B5\u4E0B\uFF0C\u6CA1\u6709\u4EBA\u80FD\u5728\u8352\u539F\u91CC\u751F\u5B58\u4E0B\u53BB\u3002"
    options: 3b0000003c000000
  - character: 1
    text: "\u53EA\u662F\u6536\u96C6\u90A3\u4E9B\u6B8B\u9AB8\u7684\u8BDD\u8FD8\u6709\u4EC0\u4E48\u610F\u4E49\u5462\uFF1F"
    options: 3d000000
  - character: 1
    text: "\u6240\u4EE5\u4F60\u73B0\u5728\u7684\u4EFB\u52A1\u662F\u4E3A\u4E86\u796D\u5960\u4ED6\u4EEC\u5417\uFF1F"
    options: 3e000000
  - character: 0
    text: "\u5F53\u7136\u662F\u6709\u610F\u4E49\u7684\u4E86\u3002"
    options: 3f000000
  - character: 0
    text: "\u5E76\u4E0D\u53EA\u662F\u4E3A\u4E86\u796D\u5960\u3002"
    options: 
  - character: 0
    text: "\u6211\u4EEC\u6700\u521D\u7684\u4EFB\u52A1\u5C31\u662F\u5C06\u51E0\u4EF6\u4EFB\u52A1\u7269\u54C1\u5B89\u5168\u8FD0\u8F93\u5230\u6307\u5B9A\u7684\u5730\u70B9\u3002"
    options: 
  - character: 0
    text: "\u73B0\u5728\u6211\u9700\u8981\u627E\u56DE\u90A3\u4E9B\u6B8B\u7559\u7684\u673A\u4F53\u4E0A\u7684\u7269\u54C1\uFF0C\u7136\u540E\u7EE7\u7EED\u8FDB\u884C\u4EFB\u52A1\u3002"
    options: 
  - character: 0
    text: "\u6211\u8FD8\u6D3B\u7740\uFF0C\u4EFB\u52A1\u8FD8\u6CA1\u6709\u5931\u8D25\u3002"
    options: 4200000043000000
  - character: 1
    text: "\u4F60\u4E3A\u4EC0\u4E48\u80FD\u4E3A\u4E86\u4EFB\u52A1\u8FD9\u4E48\u62FC\u547D\uFF1F"
    options: 44000000
  - character: 1
    text: "\u4F60\u4E0D\u5BB3\u6015\u5417\uFF1F"
    options: 45000000
  - character: 0
    text: "\u6211\u4E0D\u80FD\u8F9C\u8D1F\u6B7B\u53BB\u7684\u5927\u5BB6\u3002"
    options: 46000000
  - character: 0
    text: "\u6211\u4E0D\u5BB3\u6015\u3002"
    options: 
  - character: 0
    text: "\u2026\u2026"
    options: 
  - character: 0
    text: "\u53C8\u804A\u5230\u8FD9\u79CD\u6D88\u6781\u7684\u5730\u65B9\u4E86\uFF0C\u6765\u804A\u804ARyota\u7684\u5DE5\u4F5C\u5427\u3002"
    options: 
  - character: 0
    text: "\u867D\u7136\u4E4B\u524D\u542C\u4F60\u8BF4\u662F\u5F88\u65E0\u804A\u4EC0\u4E48\u7684\u3002"
    options: 
  - character: 0
    text: "\u4F46\u662FRyota\u7684\u5DE5\u4F5C\u5177\u4F53\u662F\u505A\u4EC0\u4E48\u7684\u5462\uFF1F"
    options: 4a0000004b000000
  - character: 1
    text: "\u5C31\u662F\u8001\u677F\u8BF4\u4EC0\u4E48\u6211\u5C31\u505A\u4EC0\u4E48\u3002"
    options: 4c000000
  - character: 1
    text: "\u5C31\u50CF\u662F\u4E00\u4E2A\u7A7A\u8F6C\u7684\u98CE\u8F66\u3002"
    options: 50000000
  - character: 0
    text: "\u90A3\u548C\u6211\u4EEC\u519B\u4EBA\u76F8\u6BD4\u662F\u4E0D\u662F\u5DEE\u4E0D\u591A\uFF1F"
    options: 
  - character: 0
    text: "\u4E0D\u8FC7\u6CA1\u6709\u866B\u65CF\u7684\u4E16\u754C\u4E5F\u9700\u8981\u4EC0\u4E48\u90FD\u670D\u4ECE\u547D\u4EE4\u7684\u8BDD\uFF0C\u90A3\u4E5F\u592A\u5947\u602A\u4E86\u5427\u3002"
    options: 
  - character: 0
    text: "\u660E\u660E\u5C31\u53EA\u662F\u4E2A\u8001\u677F\u800C\u5DF2\uFF01"
    options: 
  - character: 1
    text: "\u786E\u5B9E\u5F88\u5947\u602A\u554A\uFF01"
    options: 54000000
  - character: 0
    text: "\u6211\u77E5\u9053\u98CE\u8F66\uFF0C\u5C31\u662F\u80FD\u7528\u98CE\u529B\u53D1\u7535\u7684\u90A3\u4E2A\u5BF9\u5427\u3002"
    options: 
  - character: 0
    text: "\u6211\u4EEC\u636E\u70B9\u90A3\u8FB9\u98CE\u7279\u522B\u5927\uFF0C\u636E\u70B9\u7684\u7535\u5C31\u662F\u9760\u7684\u98CE\u8F66\u3002"
    options: 
  - character: 1
    text: "\u5458\u5DE5\u5C31\u662F\u98CE\u8F66\uFF0C\u4E0D\u505C\u5730\u7A7A\u8F6C\uFF0C\u7136\u540E\u7ED9\u8001\u677F\u53D1\u7535\u3002"
    options: 
  - character: 0
    text: "\u5458\u5DE5\u5C31\u50CF\u662F\u4E2A\u7269\u54C1\u4E00\u6837\u7684\u611F\u89C9\u5417\uFF1F"
    options: 
  - character: 0
    text: "\u542C\u8D77\u6765\u8001\u677F\u662F\u4E2A\u5F88\u574F\u7684\u5BB6\u4F19\u554A\u3002"
